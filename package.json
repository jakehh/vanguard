{
  "name": "Vanguard",
  "main": "./app/index.html",
  "description": "Digital Audio Workstation",
  "homepage": "https://github.com/jakejarrett/vanguard",
  "version": "0.0.2",
  "releaseName": "",
  "chromium-args": "--enable-webaudio-input --enable-experimental-canvas-features --enable-transparent-visuals --disable-gpu",
  "keywords": [
    "vanguard",
    "Music Production",
    "Digital Audio Workstation"
  ],
  "scripts": {
    "start": "nw"
  },
  "author": "Jake Jarrett <jakeryanjarrett@gmail.com>",
  "window": {
    "title": "Vanguard",
    "toolbar": false,
    "frame": false,
    "transparent": true,
    "icon": "./src/app/resources/img/icon.png",
    "single-instance": true,
    "width": 1400,
    "height": 800,
    "position": "center",
    "min_width": 1200,
    "min_height": 750
  },
  "js-flags": "--harmony",
  "dependencies": {
    "backbone.marionette": "^2.4.7",
    "bootstrap": "^3.3.6",
    "idom-template": "^1.1.3",
    "jquery": "^2.2.4",
    "stringify": "^5.1.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "cssnano": "^3.7.1",
    "gulp": "3.8.11",
    "gulp-install": "^0.2.0",
    "gulp-sass": "1.3.2",
    "gulp-util": "^3.0.3",
    "node-webkit-builder": "^1.0.8",
    "postcss": "^5.0.21"
  }
}
