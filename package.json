{
  "name": "Vanguard",
  "main": "./app/index.html",
  "description": "Digital Audio Workstation",
  "homepage": "https://github.com/jakejarrett/vanguard",
  "version": "0.0.1",
  "releaseName": "",
  "chromium-args": "--enable-webaudio-input --enable-experimental-canvas-features --enable-transparent-visuals --disable-gpu",
  "keywords": [
    "vanguard",
    "Music Production",
    "Digital Audio Workstation"
  ],
  "scripts": {
    "start": "nw"
  },
  "author": [
    {
      "name": "Jake Jarrett",
      "email": "jakeryanjarrett@gmail.com",
      "web": "https://github.com/jakejarrett"
    }
  ],
  "window": {
    "title": "Vanguard",
    "toolbar": false,
    "frame": false,
    "transparent": true,
    "icon": "./src/app/resources/img/icon.png",
    "single-instance": true,
    "width": 1400,
    "height": 800,
    "position": "center",
    "min_width": 1200,
    "min_height": 750
  },
  "dependencies": {
    "async": "^0.9.0",
    "i18n": "0.5.0",
    "jquery": "^2.1.3",
    "node-vst-host": "^0.0.3",
    "node-webkit-fdialogs": "latest",
    "nw": "latest",
    "q": "^1.1.2",
    "sha1": "1.1.0"
  },
  "devDependencies": {
    "gulp": "3.8.11",
    "gulp-install": "^0.2.0",
    "gulp-sass": "1.3.2",
    "gulp-util": "^3.0.3",
    "node-webkit-builder": "^1.0.8"
  }
}
